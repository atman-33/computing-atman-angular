{
  "name": "client",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "client/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/client",
        "index": "client/src/index.html",
        "main": "client/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "client/tsconfig.app.json",
        "assets": [
          "client/src/favicon.ico",
          "client/src/assets"
        ],
        "styles": [
          "node_modules/bootstrap/dist/css/bootstrap.min.css",
          "client/src/assets/scss/neumorphism.scss",
          "node_modules/prismjs/themes/prism.css",
          "node_modules/prismjs/plugins/toolbar/prism-toolbar.min.css",
          "node_modules/prismjs/plugins/line-numbers/prism-line-numbers.min.css",
          "client/src/assets/scss/blog-post.scss",
          "client/src/styles.scss"
        ],
        "scripts": [
          "node_modules/jquery/dist/jquery.min.js",
          "node_modules/popper.js/dist/umd/popper.min.js",
          "node_modules/bootstrap/dist/js/bootstrap.min.js",
          "node_modules/headroom.js/dist/headroom.js",
          "node_modules/jquery-ui/dist/jquery-ui.min.js",
          "node_modules/jquery.counterup/jquery.counterup.min.js",
          "node_modules/jquery-countdown/dist/jquery.countdown.min.js",
          "node_modules/jarallax/dist/jarallax.min.js",
          "node_modules/smooth-scroll/dist/smooth-scroll.min.js",
          "node_modules/@fortawesome/fontawesome-free/js/all.min.js",
          "node_modules/prismjs/prism.js",
          "node_modules/prismjs/plugins/toolbar/prism-toolbar.min.js",
          "node_modules/prismjs/plugins/show-language/prism-show-language.min.js",
          "node_modules/prismjs/plugins/copy-to-clipboard/prism-copy-to-clipboard.min.js",
          "node_modules/prismjs/plugins/line-numbers/prism-line-numbers.min.js",
          "client/src/assets/js/neumorphism.js"
        ],
        "allowedCommonJsDependencies": [
          "prismjs",
          "prismjs/plugins/copy-to-clipboard"
        ]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "4mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "optimization": {
            "scripts": true,
            "styles": true,
            "fonts": false
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "client:build:production"
        },
        "development": {
          "browserTarget": "client:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "client:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "client/**/*.ts",
          "client/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "client/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "client:build"
      }
    }
  }
}