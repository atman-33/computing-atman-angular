{
  "scripts": {
    "typeorm-build-entites": "npx tsc server/src/app/**/*.entity.ts --outDir ./dist/server --experimentalDecorators true --emitDecoratorMetadata",
    "typeorm-build-migrations": "npx tsc server/src/app/migrations/*.ts --outDir ./dist/server/migrations --experimentalDecorators true --emitDecoratorMetadata",
    "typeorm-migration-generate": "npm run typeorm-build-entites && npx typeorm migration:generate -f server/ormconfig.js -n",
    "typeorm-migration-run": "npm run typeorm-build-migrations && npx typeorm migration:run -f server/ormconfig.js"
  },
