{
  "name": "computing-atman",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "typeorm": "npx typeorm-ts-node-commonjs",
    "create-nest-module-controller-service": "nx g @nx/nest:module app/${npm_config_name} --project=server && nx g @nx/nest:controller app/${npm_config_name} --project=server && nx g @nx/nest:service app/${npm_config_name} --project=server",
    "tsc-entities": "npx tsc server/src/app/**/*.entity.ts --outDir ./dist/server --experimentalDecorators true --emitDecoratorMetadata",
    "tsc-migrations": "npx tsc server/src/app/migrations/*.ts --outDir ./dist/server/migrations --experimentalDecorators true --emitDecoratorMetadata",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate -d server/data-source.ts server/src/app/migrations/${npm_config_name}",
    "migration:run": "npm run tsc-migrations && npx typeorm-ts-node-commonjs migration:run -d server/data-source.ts",
    "analyze": "webpack-bundle-analyzer dist/client/stats.json",
    "build": "nx build client --prod && nx build server --prod && nx build libs --prod",
    "start": "node dist/server/main.js"
  },
